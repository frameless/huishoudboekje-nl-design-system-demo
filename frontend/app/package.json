    "fill-range": "^7.0.1",
    "framer-motion": "^4.1.11",
    "graphql": "15.5.0",
    "graphql-tools": "^7.0.4",
    "http-proxy-middleware": "^1.3.1",
    "i18next": "^20.2.2",
