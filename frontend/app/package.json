    "react": "^17.0.2",
    "react-datepicker": "^3.8.0",
    "react-dom": "^17.0.2",
    "react-google-charts": "^3.0.15",
    "react-grapple": "^1.6.0",
    "react-i18next": "^11.8.15",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
