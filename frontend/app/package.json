{
  "name": "huishoudboekje",
  "version": "0.21.0",
  "author": {
    "name": "Koen Brouwer",
    "email": "koen.brouwer@vng.nl"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/commonground/huishoudboekje/app-new"
  },
  "bugs": {
    "url": "https://gitlab.com/commonground/huishoudboekje/app-new/-/issues"
  },
  "scripts": {
    "postinstall": "node version.js",
    "prestart": "./prestart.sh",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "pretest": "npm run i18n-migrate",
    "test": "react-scripts test",
    "test:coverage": "npm run test -- --coverage --reporters=default --reporters=jest-junit",
    "eject": "react-scripts eject",
    "cypress": "npx -q cypress open",
    "theme": "./setTheme.sh",
    "lint": "eslint src",
    "get-schema": "npx -q apollo client:download-schema --endpoint=${BASE_URL:-http://localhost:3000}/api/graphql schema.graphql",
    "pregen-types": "npm run get-schema",
    "gen-types": "graphql-codegen --config codegen.json",
    "i18n-migrate": "i18next 'src/**/*.{ts,tsx}' [-oc]"
  },
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "@chakra-ui/icons": "^1.0.2",
    "@chakra-ui/react": "^1.1.2",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@types/react-datepicker": "^3.1.2",
    "apollo-upload-client": "^14.1.3",
    "array-to-sentence": "^2.0.0",
    "date-fns": "^2.16.1",
    "framer-motion": "^3.1.4",
    "graphql": "14.7.0",
    "http-proxy-middleware": "^1.0.6",
    "i18next": "^19.8.2",
    "ibantools": "^3.2.2",
    "mobx": "^6.0.4",
    "mobx-react": "^7.0.5",
    "mobx-state-tree": "^4.0.2",
    "moment": "^2.29.1",
    "moment-recur-ts": "^1.3.1",
    "pretty-bytes": "^5.5.0",
    "react": "^17.0.1",
    "react-datepicker": "^3.3.0",
    "react-dom": "^17.0.1",
    "react-google-charts": "^3.0.15",
    "react-grapple": "^1.5.1",
    "react-i18next": "^11.8.5",
    "react-icons": "^3.11.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.0",
    "react-select": "^3.1.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.19.4",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typescript": "1.19.0",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-select": "^3.1.2",
    "@typescript-eslint/parser": "^4.12.0",
    "browserslist": "^4.16.0",
    "cypress-graphql-mock": "0.3.1",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-react-app": "^6.2.2",
    "i18next-parser": "^3.6.0",
    "jest-junit": "^12.0.0"
  },
  "browserslist": [
    "> 0.5%",
    "not ie 11"
  ],
  "jest": {
    "coverageReporters": [
      "text",
      "cobertura"
    ]
  }
}
