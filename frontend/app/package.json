  "scripts": {
    "postinstall": "node version.js",
    "prestart": "./prestart.sh",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "pretest": "npm run i18n-migrate",
    "test": "react-scripts test",
    "test:coverage": "npm run test -- --coverage --reporters=default --reporters=jest-junit",
    "eject": "react-scripts eject",
    "cypress": "npx -q cypress open",
    "theme": "./setTheme.sh",
    "lint": "eslint src",
    "get-schema": "npx --yes apollo client:download-schema ${PROXY_AUTHORIZATION:+--header \"${PROXY_AUTHORIZATION}\"} --endpoint=${BASE_URL:-http://localhost:3000}/api/graphql schema.graphql",
    "pregen-types": "npm run get-schema",
    "gen-types": "graphql-codegen --config codegen.json",
    "i18n-migrate": "i18next 'src/**/*.{ts,tsx}' [-oc]"
  },
  "dependencies": {
    "@apollo/client": "^3.3.16",
    "@chakra-ui/icons": "^1.0.13",
