{
  "name": "huishoudboekje",
  "version": "2.1.1",
  "author": {
    "name": "Koen Brouwer",
    "email": "contact@kbrouwer.nl",
    "url": "https://gitlab.com/koenbrouwer"
  },
  "private": true,
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/commonground/huishoudboekje/app-new"
  },
  "bugs": {
    "url": "https://gitlab.com/commonground/huishoudboekje/app-new/-/issues"
  },
  "scripts": {
    "postinstall": "node version.js",
    "prestart": "sh ./prestart.sh",
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "winBuild": "set \"GENERATE_SOURCEMAP=false\" && react-scripts build",
    "pretest": "npm run i18n-migrate",
    "test": "react-scripts test",
    "testleak": "react-scripts test --detectOpenHandles",
    "test:coverage": "npm run test -- --coverage --testResultsProcessor=jest-junit",
    "eject": "react-scripts eject",
    "theme": "./setTheme.sh",
    "lint": "eslint src",
    "get-schema": "rover graph introspect http://localhost:4000/apiV2/graphql > schema.graphql",
    "pregen-types": "npm run get-schema",
    "gen-types": "graphql-codegen --verbose --config graphql-codegen.yaml",
    "i18n-migrate": "i18next 'src/**/*.{ts,tsx}' [-oc]"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "dependencies": {
    "@apollo/client": "^3.7.17",
    "@chakra-ui/icons": "^2.0.13",
    "@chakra-ui/react": "^2.4.3",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@storybook/addon-docs": "^7.1.1",
    "@tanstack/react-query": "^4.19.1",
    "@types/react-datepicker": "^4.14.1",
    "apollo-link-debounce": "^3.0.0",
    "apollo-upload-client": "^17.0.0",
    "array-to-sentence": "^2.0.0",
    "currency.js": "^2.0.4",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.7",
    "dayjs-recur": "^1.0.1",
    "fill-range": "^7.0.1",
    "framer-motion": "^10.13.0",
    "graphql": "^16.8.1",
    "graphql-sse": "^2.5.3",
    "graphql-tools": "^8.3.14",
    "graphql-ws": "^5.16.0",
    "http-proxy-middleware": "^2.0.6",
    "i18next": "^23.7.11",
    "ibantools": "^4.2.1",
    "jsonpath": "^1.1.1",
    "lodash": "^4.17.21",
    "postcss": "^8.4.19",
    "react": "^18.2.0",
    "react-datepicker": "^4.14.1",
    "react-dom": "^18.2.0",
    "react-google-charts": "^4.0.0",
    "react-i18next": "^13.5.0",
    "react-icons": "^4.10.1",
    "react-router-dom": "^6.14.2",
    "react-scripts": "^5.0.1",
    "react-select": "^5.7.0",
    "sass": "^1.64.0",
    "typescript": "^5.1.6",
    "ua-parser-js": "^1.0.32",
    "zod": "^3.22.1",
    "zustand": "^4.1.5"
  },
  "devDependencies": {
    "@apollo/rover": "^0.23.0",
    "@chakra-ui/storybook-addon": "^4.0.14",
    "@clarketm/jwt-cli": "^1.2.0",
    "@graphql-codegen/cli": "^2.16.1",
    "@graphql-codegen/introspection": "2.2.3",
    "@graphql-codegen/typescript": "^2.8.5",
    "@graphql-codegen/typescript-operations": "^2.5.10",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.12",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-router-dom": "^5.3.3",
    "@types/ua-parser-js": "^0.7.36",
    "babel-eslint": "^10.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.14",
    "i18next-parser": "^8.13.0",
    "jest-junit": "^16.0.0",
    "nwsapi": "2.2.2",
    "webpack": "^5.82.0"
  },
  "overrides": {
    "loader-utils@^2.0.0 <2.0.3": "^2.0.3",
    "nth-check": "^2.0.1"
  },
  "browserslist": [
    "> 0.5%",
    "not ie 11"
  ],
  "jest": {
    "coverageReporters": [
      "cobertura",
      "text"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.{test,spec}.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ]
  }
}
