{
  "name": "huishoudboekje",
  "version": "1.2.2",
  "author": {
    "name": "Koen Brouwer",
    "email": "contact@kbrouwer.nl",
    "url": "https://gitlab.com/koenbrouwer"
  },
  "private": true,
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/commonground/huishoudboekje/app-new"
  },
  "bugs": {
    "url": "https://gitlab.com/commonground/huishoudboekje/app-new/-/issues"
  },
  "scripts": {
    "postinstall": "node version.js",
    "prestart": "sh ./prestart.sh",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "pretest": "npm run i18n-migrate",
    "test": "react-scripts test",
    "test:coverage": "npm run test -- --coverage --testResultsProcessor=jest-junit",
    "eject": "react-scripts eject",
    "theme": "./setTheme.sh",
    "lint": "eslint src",
    "get-schema": "rover graph introspect http://localhost:3000/api/graphql > schema.graphql",
    "pregen-types": "npm run get-schema",
    "gen-types": "graphql-codegen --config graphql-codegen.yaml",
    "i18n-migrate": "i18next 'src/**/*.{ts,tsx}' [-oc]",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@chakra-ui/icons": "^2.0.11",
    "@chakra-ui/react": "^2.3.5",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@storybook/addon-docs": "^6.5.12",
    "@tanstack/react-query": "^4.10.3",
    "@types/react-datepicker": "^4.4.2",
    "apollo-link-debounce": "^3.0.0",
    "apollo-upload-client": "^17.0.0",
    "array-to-sentence": "^2.0.0",
    "currency.js": "^2.0.4",
    "date-fns": "^2.29.3",
    "dayjs": "^1.11.5",
    "dayjs-recur": "^1.0.1",
    "fill-range": "^7.0.1",
    "framer-motion": "^7.5.3",
    "graphql": "^16.6.0",
    "graphql-tools": "^8.3.6",
    "http-proxy-middleware": "^2.0.6",
    "i18next": "^21.10.0",
    "ibantools": "^4.1.6",
    "jsonpath": "^1.1.1",
    "postcss": "^8.4.17",
    "react": "^18.2.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.2.0",
    "react-google-charts": "^4.0.0",
    "react-i18next": "^11.18.6",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.4.2",
    "react-scripts": "^5.0.1",
    "react-select": "^5.4.0",
    "sass": "^1.55.0",
    "typescript": "^4.8.4",
    "ua-parser-js": "^1.0.2",
    "zod": "^3.19.1",
    "zustand": "^4.1.2"
  },
  "devDependencies": {
    "@apollo/rover": "^0.9.1",
    "@chakra-ui/storybook-addon": "^4.0.12",
    "@clarketm/jwt-cli": "^1.2.0",
    "@graphql-codegen/cli": "^2.13.5",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-react-apollo": "^3.3.3",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/node-logger": "^6.5.12",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.12",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/ua-parser-js": "^0.7.36",
    "babel-eslint": "^10.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "i18next-parser": "^6.6.0",
    "jest-junit": "^14.0.1",
    "storybook-dark-mode": "^1.1.2",
    "storybook-react-i18next": "^1.1.2",
    "webpack": "^5.74.0"
  },
  "browserslist": [
    "> 0.5%",
    "not ie 11"
  ],
  "jest": {
    "coverageReporters": [
      "cobertura",
      "text"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.{test,spec}.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ]
  }
}
