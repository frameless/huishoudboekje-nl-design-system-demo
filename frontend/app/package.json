{
  "name": "huishoudboekje",
  "version": "0.21.0",
  "author": {
    "name": "Koen Brouwer",
    "email": "koen.brouwer@vng.nl"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/commonground/huishoudboekje/app-new"
  },
  "bugs": {
    "url": "https://gitlab.com/commonground/huishoudboekje/app-new/-/issues"
  },
  "scripts": {
    "postinstall": "node version.js",
    "prestart": "./prestart.sh",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "pretest": "npm run i18n-migrate",
    "test": "react-scripts test",
    "test:coverage": "npm run test -- --coverage --reporters=default --reporters=jest-junit",
    "eject": "react-scripts eject",
    "cypress": "npx -q cypress open",
    "theme": "./setTheme.sh",
    "lint": "eslint src",
    "get-schema": "npx -q apollo client:download-schema --endpoint=${BASE_URL:-http://localhost:3000}/api/graphql schema.graphql",
    "pregen-types": "npm run get-schema",
    "gen-types": "graphql-codegen --config codegen.json",
    "i18n-migrate": "i18next 'src/**/*.{ts,tsx}' [-oc]"
  },
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "@chakra-ui/icons": "^1.0.5",
    "@chakra-ui/react": "^1.3.3",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@types/react-datepicker": "^3.1.3",
    "apollo-upload-client": "^14.1.3",
    "array-to-sentence": "^2.0.0",
    "date-fns": "^2.17.0",
    "framer-motion": "^3.3.0",
    "graphql": "14.7.0",
    "http-proxy-middleware": "^1.0.6",
    "i18next": "^19.8.8",
    "ibantools": "^3.2.3",
    "moment": "^2.29.1",
    "moment-recur-ts": "^1.3.1",
    "pretty-bytes": "^5.5.0",
    "react": "^17.0.1",
    "react-datepicker": "^3.4.1",
    "react-dom": "^17.0.1",
    "react-google-charts": "^3.0.15",
    "react-grapple": "^1.5.1",
    "react-i18next": "^11.8.7",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.2",
    "react-select": "^4.1.0",
    "typescript": "^4.1.5",
    "ua-parser-js": "^0.7.24"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.20.1",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typescript": "^1.21.0",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.28",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-select": "^4.0.13",
    "@types/ua-parser-js": "^0.7.35",
    "@typescript-eslint/parser": "^4.15.1",
    "browserslist": "^4.16.3",
    "cypress": "^6.5.0",
    "cypress-graphql-mock": "0.3.1",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-react-app": "^6.2.2",
    "i18next-parser": "^3.6.0",
    "jest-junit": "^12.0.0"
  },
  "browserslist": [
    "> 0.5%",
    "not ie 11"
  ],
  "jest": {
    "coverageReporters": [
      "text",
      "cobertura"
    ]
  }
