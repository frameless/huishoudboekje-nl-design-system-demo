{
  "name": "huishoudboekje",
  "version": "1.1.2",
  "author": {
    "name": "Koen Brouwer",
    "email": "contact@kbrouwer.nl",
    "url": "https://gitlab.com/koenbrouwer"
  },
  "private": true,
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/commonground/huishoudboekje/app-new"
  },
  "bugs": {
    "url": "https://gitlab.com/commonground/huishoudboekje/app-new/-/issues"
  },
  "scripts": {
    "postinstall": "node version.js",
    "prestart": "sh ./prestart.sh",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "pretest": "npm run i18n-migrate",
    "test": "react-scripts test",
    "test:coverage": "npm run test -- --coverage --testResultsProcessor=jest-junit",
    "eject": "react-scripts eject",
    "theme": "./setTheme.sh",
    "lint": "eslint src",
    "get-schema": "rover graph introspect ${PROXY_AUTHORIZATION:+--header \"${PROXY_AUTHORIZATION}\"} ${BASE_URL:-http://localhost:3000}/api/graphql > schema.graphql",
    "pregen-types": "npm run get-schema",
    "gen-types": "graphql-codegen --config graphql-codegen.yaml",
    "i18n-migrate": "i18next 'src/**/*.{ts,tsx}' [-oc]",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@apollo/client": "3.5.10",
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.8.8",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@koenbrouwer/find-path-by-value": "^1.0.0",
    "@storybook/addon-docs": "^6.5.8",
    "@types/react-datepicker": "^4.4.2",
    "apollo-link-debounce": "^3.0.0",
    "apollo-upload-client": "^17.0.0",
    "array-to-sentence": "^2.0.0",
    "currency.js": "^2.0.4",
    "date-fns": "^2.28.0",
    "dayjs": "^1.11.3",
    "dayjs-recur": "^1.0.1",
    "eslint-config-react-app": "^7.0.1",
    "fill-range": "^7.0.1",
    "framer-motion": "^4.1.17",
    "graphql": "^16.5.0",
    "graphql-tools": "^8.2.12",
    "http-proxy-middleware": "^2.0.6",
    "i18next": "^21.8.9",
    "ibantools": "^4.1.5",
    "postcss": "^8.4.14",
    "react": "^17.0.2",
    "react-datepicker": "^4.8.0",
    "react-dom": "^17.0.2",
    "react-google-charts": "^4.0.0",
    "react-i18next": "^11.17.1",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-select": "^5.3.2",
    "sass": "^1.52.3",
    "typescript": "^4.7.3",
    "ua-parser-js": "^1.0.2",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@apollo/rover": "^0.7.0",
    "@chakra-ui/storybook-addon": "^1.0.3",
    "@clarketm/jwt-cli": "^1.2.0",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "^2.5.1",
    "@graphql-codegen/typescript-operations": "^2.4.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.16",
    "@storybook/addon-actions": "^6.5.8",
    "@storybook/addon-essentials": "^6.5.8",
    "@storybook/addon-links": "^6.5.8",
    "@storybook/builder-webpack5": "^6.5.8",
    "@storybook/manager-webpack5": "^6.5.8",
    "@storybook/node-logger": "^6.5.8",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.8",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^28.1.1",
    "@types/node": "^16.11.27",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-router-dom": "^5.3.3",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "babel-eslint": "^10.1.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-app": "^6.2.2",
    "eslint-plugin-react-hooks": "^4.5.0",
    "i18next-parser": "^6.5.0",
    "jest-junit": "^13.2.0",
    "storybook-dark-mode": "^1.1.0",
    "storybook-react-i18next": "^1.0.20",
    "webpack": "^5.73.0"
  },
  "browserslist": [
    "> 0.5%",
    "not ie 11"
  ],
  "jest": {
    "coverageReporters": [
      "cobertura",
      "text"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.{test,spec}.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
