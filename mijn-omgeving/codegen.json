{
  "overwrite": true,
  "schema": "schema.graphql",
  "documents": "./src/lib/graphql/**",
  "generates": {
	"./src/lib/generated/graphql.tsx": {
	  "plugins": [
		"typescript",
		"typescript-operations",
		"typescript-react-apollo"
	  ]
	}
  },
  "config": {
	"maybeValue": "T",
	"defaultScalarType": "any",
	"skipTypename": true
  }
}