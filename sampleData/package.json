{
  "name": "sampledata",
  "version": "1.0.0",
  "description": "Een tool waarmee data in Huishoudboekje kan worden geladen.",
  "main": "main.ts",
  "author": {
	"name": "Koen Brouwer",
	"email": "contact@kbrouwer.nl",
	"url": "https://gitlab.com/koenbrouwer"
  },
  "keywords": [],
  "license": "SEE LICENSE IN LICENSE.md",
  "engines": {
	"node": ">=18"
  },
  "scripts": {
	"start": "ts-node main.ts",
	"dev": "nodemon main.ts",
	"test": "jest",
	"test-dev": "jest --watch",
	"get-schema": "npx --yes -p @apollo/rover rover graph introspect ${PROXY_AUTHORIZATION:+--header \"${PROXY_AUTHORIZATION}\"} ${BASE_URL:-http://localhost:3000}/api/graphql > schema.graphql",
	"pregen-types": "npm run get-schema",
	"gen-types": "graphql-codegen --config codegen.json"
  },
  "dependencies": {
	"@apollo/client": "^3.6.9",
	"@graphql-codegen/cli": "^2.9.1",
	"@graphql-codegen/introspection": "2.2.0",
	"@graphql-codegen/typescript": "^2.7.2",
	"@graphql-codegen/typescript-generic-sdk": "^2.4.2",
	"@graphql-codegen/typescript-operations": "^2.5.2",
	"@types/jest": "^28.1.6",
	"@types/node": "^18.6.1",
	"apollo-link-error": "^1.1.13",
	"apollo-upload-client": "^17.0.0",
	"cross-fetch": "^3.1.5",
	"dotenv": "^16.0.1",
	"graphql": "^16.5.0",
	"jest": "^28.1.3",
	"nodemon": "^2.0.19",
	"ts-jest": "^28.0.7",
	"ts-node": "^10.9.1",
	"typescript": "^4.7.4",
	"yaml": "^2.1.1"
  }
}
