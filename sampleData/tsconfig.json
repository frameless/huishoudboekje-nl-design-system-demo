{
  "type": "module",
  "module": "esnext",
  "compilerOptions": {
	"target": "es5",
	"lib": [
	  "esnext", "webworker"
	],
	"allowJs": true,
	"skipLibCheck": true,
	"esModuleInterop": true,
	"allowSyntheticDefaultImports": true,
	"strict": true,
	"forceConsistentCasingInFileNames": true,
	"moduleResolution": "node",
	"resolveJsonModule": true,
	"isolatedModules": true,
	"noEmit": true,
	"noImplicitAny": false,
	"noFallthroughCasesInSwitch": true
  },
  "include": [
	"*.ts",
	"src"
  ]
}
