datasource db {
  provider      = "postgresql"
  url           = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl", "linux-arm64-openssl-1.1.x"]
}

model Alarm {
	id 					String		@id @default(uuid())
	gebruikerEmail		String
	afspraakId			Int
	signaalId			String?
	isActive			Boolean
	datum				String
	datumMargin			Int
	bedrag				Int
	bedragMargin		Int
}
