{
  "name": "postadressenservice",
  "version": "0.1.0",
  "description": "Little NodeJS Express server that handles requests for saving information about postal addresses.",
  "main": "main.ts",
  "scripts": {
    "dev": "nodemon -r dotenv/config",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev",
    "db:deploy": "npx prisma migrate deploy",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "prestart": "npx prisma generate",
    "start": "ts-node -r dotenv/config main.ts"
  },
  "author": {
    "name": "Koen Brouwer",
    "email": "contact@kbrouwer.nl",
    "url": "https://gitlab.com/koenbrouwer"
  },
  "engines": {
    "node": ">=16 <18"
  },
  "dependencies": {
    "@prisma/client": "^3.15.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "nodemon": "^2.0.16",
    "prisma": "^3.15.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.1",
    "@types/supertest": "^2.0.12",
    "eslint": "^7.32.0",
    "jest": "^28.1.1",
    "jest-junit": "^13.2.0",
    "jest-mock-extended": "^2.0.6",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.5"
  },
  "keywords": [],
  "license": "SEE LICENSE IN LICENSE.md"
}
