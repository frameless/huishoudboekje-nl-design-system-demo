datasource db {
  provider      = "postgresql"
  url           = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl", "linux-arm64-openssl-1.1.x"]
}

model Address {
	id 					String		@id @default(uuid())
	street				String?
	houseNumber			String?
	postalCode			String?
	locality			String?
	timeCreated			DateTime	@default(now())
}
