datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Address {
	id 					String		@id @default(uuid())
	street				String?
	houseNumber			String?
	postalCode			String?
	locality			String?
	timeCreated			DateTime	@default(now())
}
