datasource db {
  provider      = "postgresql"
  url           = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl", "linux-arm64-openssl-1.1.x", "darwin"]
}

model Signal {
	id 					String		@id @default(uuid())
	alarmId				String
	alarmUuid			String
	banktransactieIds	Int[]
	banktransactieUuids	String[]
	isActive			Boolean
	type				String
	actions				String[]
	context				Json?
	bedragDifference	String?
	timeUpdated			DateTime	@updatedAt
}
